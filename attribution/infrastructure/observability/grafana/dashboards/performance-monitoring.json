{
  "dashboard": {
    "id": null,
    "uid": "unmogrowp-performance",
    "title": "UnMoGrowP Performance Monitoring - Week 1 Sprint",
    "tags": ["unmogrowp", "performance", "week1", "sprint"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "editable": true,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "Event Processing Rate (Target: 1M+ events/sec)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(attribution_events_processed_total[1m]))",
            "legendFormat": "Events/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 500000},
                {"color": "green", "value": 1000000}
              ]
            },
            "mappings": [],
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "P95 API Latency (Target: <100ms)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(attribution_event_processing_duration_seconds_bucket[5m])) by (le)) * 1000",
            "legendFormat": "P95 Latency",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 100}
              ]
            },
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "System Uptime (Target: >99.9%)",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(up{job=~\"unmogrowp.*\"}) * 100",
            "legendFormat": "Uptime %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 99.0},
                {"color": "green", "value": 99.9}
              ]
            },
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Attribution Accuracy (Target: >99%)",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(attribution_accuracy_percent)",
            "legendFormat": "Accuracy %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Event Processing Rate Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(attribution_events_processed_total[1m]))",
            "legendFormat": "Events/sec",
            "refId": "A"
          },
          {
            "expr": "1000000",
            "legendFormat": "Target (1M/sec)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "Events per second",
            "format": "ops",
            "min": 0
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 6,
        "title": "API Latency Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(attribution_event_processing_duration_seconds_bucket[5m])) by (le)) * 1000",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(attribution_event_processing_duration_seconds_bucket[5m])) by (le)) * 1000",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(attribution_event_processing_duration_seconds_bucket[5m])) by (le)) * 1000",
            "legendFormat": "P99",
            "refId": "C"
          },
          {
            "expr": "100",
            "legendFormat": "Target (100ms)",
            "refId": "D"
          }
        ],
        "yaxes": [
          {
            "label": "Latency (ms)",
            "format": "ms",
            "min": 0
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6}
      },
      {
        "id": 7,
        "title": "Events by Platform",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(attribution_events_processed_total[1m])) by (platform)",
            "legendFormat": "{{platform}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Events/sec",
            "format": "ops",
            "min": 0
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "lines": true,
        "stack": true,
        "fill": 5,
        "linewidth": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14}
      },
      {
        "id": 8,
        "title": "Events by Type",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(attribution_events_processed_total[1m])) by (event_type)",
            "legendFormat": "{{event_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Events/sec",
            "format": "ops",
            "min": 0
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "lines": true,
        "stack": true,
        "fill": 5,
        "linewidth": 1,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14}
      },
      {
        "id": 9,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(attribution_events_processed_total{status=\"error\"}[5m])) / sum(rate(attribution_events_processed_total[5m])) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Error Rate %",
            "format": "percent",
            "min": 0,
            "max": 5
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [1],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22}
      },
      {
        "id": 10,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "attribution_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory (MB)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Memory (MB)",
            "format": "decmbytes",
            "min": 0
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22}
      },
      {
        "id": 11,
        "title": "Active Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "attribution_active_connections",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Connections",
            "format": "short",
            "min": 0
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30}
      },
      {
        "id": 12,
        "title": "Service Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"unmogrowp.*\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "value",
          "colorMode": "background"
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30}
      }
    ],
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "name": "Alerts",
          "step": "60s",
          "tagKeys": "alertname",
          "textFormat": "{{alertname}}",
          "titleFormat": "Alert"
        }
      ]
    },
    "templating": {
      "list": []
    }
  }
}

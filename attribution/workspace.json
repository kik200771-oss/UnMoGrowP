{
  "name": "unmogrowp-attribution-platform",
  "version": "0.2.0",
  "description": "Enterprise Mobile Attribution Platform - Monorepo",
  "workspaces": [
    "apps/*",
    "services/*",
    "tools/*"
  ],
  "private": true,
  "scripts": {
    "build": "bun run --filter='*' build",
    "test": "bun run --filter='*' test",
    "dev": "bun run --filter='*' dev",
    "clean": "bun run --filter='*' clean",
    "setup": "./scripts/setup.sh"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/attribution.git"
  }
}